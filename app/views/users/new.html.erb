<h1>Users#new</h1>
<%= form_for @user do |f| %>
<% if @user.errors.any? %>
<ul>
	<% @user.errors.full_messages.each do |msg| %>
	<li><%= msg %></li>
	<% end %>
</ul>
<% end %>
<div>
<%= f.label :email %><br>
<%= f.text_field :email %>
</div>
<div>
<%= f.label :name %><br>
<%= f.text_field :name %>
</div>
<%= f.fields_for :license do |license_builder| %>
<div>
	<%= license_builder.label :number %><br>
	<%= license_builder.text_field :number %>
</div>
<div>
<%= license_builder.label :state %><br>
<%= license_builder.text_field :state %>
</div>
<% end %>
<%= f.submit %>
<% end %>
